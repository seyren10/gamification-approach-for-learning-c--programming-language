    avatar: {fileID: 21300000, guid: 66c8cabb0f3898d4786d9f50aa3a4a40, type: 3}
    tutorialAction: 0
    enablePointer: 1
    continueTrigger: {fileID: 0}
    pointerPosition: {x: -54.5, y: -177}
    fadePanelPosition: {x: 217.93, y: -314.72}
    fadePanelSize: {x: 356.91, y: 449.89}
  - speakerName: Robo-C#
    message: That's it. Now <#FF7739>close</color> this panel and let's talk about
