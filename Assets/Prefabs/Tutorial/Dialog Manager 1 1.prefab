    avatar: {fileID: 21300000, guid: 6d640965f7092cf429ea3e9d3c57356a, type: 3}
    tutorialAction: 0
    enablePointer: 1
    continueTrigger: {fileID: 0}
    pointerPosition: {x: 96, y: 405.7}
    fadePanelPosition: {x: 217.93, y: 355.8}
    fadePanelSize: {x: 356.91, y: 176.4}
